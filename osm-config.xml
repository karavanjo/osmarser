<?xml version="1.0" encoding="utf-8" ?>
<osm>
	<!-- Configures database SQL Server -->
	<database>
		<connectionString name="osmarserDB" />
		<tables>
			<geo name="dbo.Geo"/>
			<values name="dbo.TagsValues"/>
		</tables>
	</database>

	<!-- Configures import object on the basis of the presence or absence of tags in database SQL Server -->
	<tags>
		<import>		
			<string>
				<t contain="name:"/>
				<t contain="_name"/>
				<t contain="wikipedia:"/>
				<t contain="teryt:"/>
				<t contain="is_in:"/>
				<t contain="addr:house"/>
				<t contain="terrace:"/>
				<t contain="name_"/>
				<t contain="_ref"/>
				<t contain="traffic_sign:"/>
				<t contain="contact:"/>
				<t tag="name"/>
				<t tag="is_in"/>
				<t tag="addr:street"/>
				<t tag="wikipedia"/>
				<t tag="network"/>
				<t tag="town"/>
				<t tag="website"/>
				<t tag="ref"/>
				<t tag="ele"/>
				<t tag="url"/>
				<t tag="operator"/>
				<t tag="email"/>
				<t tag="phone"/>
				<t tag="fax"/>
				<t tag="opening_hours"/>
				<t tag="traffic_sign"/>
				<t tag="closest_town"/>
				<t tag="image"/>
				<t tag="branch"/>
				<t tag="brand"/>
				<t tag="pk"/>
				<t tag="addr"/>
				<t tag="country"/>
			</string>
			<int>
				<t tag="population"/>
				<t tag="building:buildyear"/>
				<t tag="admin_level"/>
				<t tag="postal_code"/>
				<t tag="layer"/>
				<t tag="stars"/>
			</int>
		</import>
		<noimport>
			<t contain="source:"/>
			<t contain="cladr:"/>
			<t contain="description:"/>
			<t contain="fixme"/>			
			<t contain="note:"/>
			<t contain="millennium:"/>
			<t contain="gns:"/>
			<t tag="FIXME"/>
			<t tag="description"/>
			<t tag="created_by"/>
			<t tag="note"/>
			<t tag="source"/>
			<t tag="addr:city"/>
			<t tag="addr:country"/>
			<t tag="addr:district"/>
			<t tag="addr:region"/>
			<t tag="addr:postcode"/>			
			<t tag="addr:full"/>
			<t tag="garmin_type"/>
			<t tag="comment"/>
		</noimport>
	</tags>

	<!--
	Configures import geographic data types in database SQL Server
	By default, the NODES are imported as a POINT,
				the WAYS are imported as a not closed - as LINES are closed as POLYGON,
				the RELATIONS are imported as a POLYGON -->
	<geo>
		<nodes>
		</nodes>
		<ways>
			<t tag="higway" geography="LINE"/>
			<t tag="building" geography="POLYGON"/>
		</ways>
		<relations>
		</relations>
	</geo>
</osm>